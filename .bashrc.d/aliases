# User specific aliases and functions
alias ..="cd .."
alias h="history"
alias diff="diff -y"
alias pf="ps -eF"
alias psl="ps -eLf"
alias pe="ps -e"
alias j="jobs"
alias df="df -h"
alias rm="rm -i"
alias l.='ls -d .*'
alias l.l='ls -d -l .*'
alias ll='ls -l'
alias ls='ls -F'
alias la='ls -a'
#alias ls='ls -F --color'
alias gitlog='git log --oneline'
alias py39='conda activate py39'
alias dgui='python $DYNAC_HOME/dgui/dgui.py -p$DYNAC_HOME/dgui'
#alias pyOR='source ~/pyOrbit/activate'
alias lspath='~/bin/path.awk'
#alias sns='cd /home/wdklotz/examples-master/SNS_Linac/pyorbit_linac_model'
alias explorer="explorer.exe ."
alias os_rel='less /etc/os-release'
alias dcls='docker container list --format "table {{.ID}}\t{{.Image}}\t{{.Names}}\t{{.Ports}}"'
alias dcll='docker container list'
alias dils='docker image ls'
alias dvls='docker volume ls'
alias dnls='docker network ls'
alias tail='tail -n 25'
alias vi='vim'
alias tao='tao -geometry 1000x600 '
alias 7768='ssh -X wdklotz@77.68.74.72'
alias cld='for I in *; do /usr/bin/rm $I; done'

envg(){
env|grep --color $1
}

