alias explorer="explorer.exe ."
#unalias vi

A() {
export A=`pwd`
}
cda(){
cd $A
}
B() {
export B=`pwd`
}
cdb(){
cd $B
}
C() {
export C=`pwd`
}
cdc(){
cd $C
}
abc(){
echo A=$A
echo B=$B
echo C=$C
}
envg(){
env|grep --color $1
}

