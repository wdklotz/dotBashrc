export EDITOR=vim
export PAGER="less -X -R -F"

A() { export A=`pwd`; }
cda() { cd $A; }
B() { export B=`pwd`; }
cdb() { cd $B; }
C() { export C=`pwd`; }
cdc() { cd $C; }
abc() { echo A=$A;echo B=$B;echo C=$C; }

envg(){
env|grep --color $1
}

if [ -f /etc/bash_completion ] && ! shopt -oq posix; then
    . /etc/bash_completion
fi

