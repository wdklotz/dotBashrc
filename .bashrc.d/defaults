export EDITOR=vim
export PAGER="less -X -R -F"

mA() { export A=`pwd`; }
cda() { cd $A; }
mB() { export B=`pwd`; }
cdb() { cd $B; }
mC() { export C=`pwd`; }
cdc() { cd $C; }
abc() { echo A=$A;echo B=$B;echo C=$C; }

if [ -f /etc/bash_completion ] && ! shopt -oq posix; then
    . /etc/bash_completion
fi

