export EDITOR=vim
export PAGER="less -X -R -F"

A() { export A=`pwd`; }
cda() { cd $A; }
B() { export B=`pwd`; }
cdb() { cd $B; }
C() { export C=`pwd`; }
cdc() { cd $C; }
abc() { echo A=$A;echo B=$B;echo C=$C; }

if [ -f /etc/bash_completion ] && ! shopt -oq posix; then
    . /etc/bash_completion
fi

# MY bin
export PATH=~wdklotz/bin:$PATH

# X11 DISPLAY
export DISPLAY=192.168.1.52:0.0

